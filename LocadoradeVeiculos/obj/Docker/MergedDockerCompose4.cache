networks:
  dockersql-network:
    driver: bridge
services:
  locadoradeveiculos:
    build:
      context: C:\Users\Freway\Documents\GitHub\Microservice\LocadoradeVeiculos
      dockerfile: LocadoradeVeiculos/LocadoradeVeiculos/Dockerfile
    depends_on:
      sql.database:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: LocadoradeVeiculos
    networks:
      dockersql-network: {}
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Freway\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\Freway\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
  sql.database:
    image: microsoft/mssql-server-windows:2019-latest
    networks:
      dockersql-network: {}
version: '3.4'